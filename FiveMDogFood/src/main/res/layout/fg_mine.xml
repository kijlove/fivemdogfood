<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".ui.fivem.main.ui.mine.FgMine"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/black"
        android:orientation="vertical">

        <TextView
            android:id="@+id/addDateSm"
            style="@style/defaut_text"
            android:layout_width="match_parent"
            android:layout_marginBottom="1px"
            android:background="@color/white"
            android:drawableRight="@drawable/ic_baseline_keyboard_arrow_right_24"
            android:text="添加数据字母"></TextView>

        <TextView
            android:id="@+id/addDateSameSex"
            style="@style/defaut_text"
            android:layout_width="match_parent"
            android:layout_marginBottom="1px"
            android:background="@color/white"
            android:drawableRight="@drawable/ic_baseline_keyboard_arrow_right_24"
            android:text="添加数据同性"></TextView>

        <TextView
            android:id="@+id/addDateOtherSex"
            style="@style/defaut_text"
            android:layout_width="match_parent"
            android:layout_marginBottom="1px"
            android:background="@color/white"
            android:drawableRight="@drawable/ic_baseline_keyboard_arrow_right_24"
            android:text="添加数据异性"></TextView>
    </LinearLayout>

</LinearLayout>